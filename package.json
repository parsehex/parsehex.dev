{
  "name": "parsehex-dev",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "new": "vite-node scripts/new.ts",
    "inbox": "vite-node scripts/inbox.ts",
    "think": "vite-node scripts/think.ts",
    "now:mac": "node --print \"Date.now()\" | tr -d '[:space:]' | pbcopy",
    "now:win": "node --print \"Date.now()\" | % { $_.Trim() } | clip",
    "editor": "cd editor && npm run dev",
    "cms": "concurrently npm:dev npm:editor"
  },
  "dependencies": {
