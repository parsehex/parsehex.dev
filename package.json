    "@iconify-json/ri": "^1.2.6",
    "@orama/orama": "^3.1.16",
    "astro": "^5.14.4",
    "astro-icon": "^1.1.5",
    "astro-seo": "^0.8.4",
    "bulma": "^1.0.4",
    "sass": "^1.93.2",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@inquirer/prompts": "^8.2.0",
    "@types/yargs": "^17.0.35",
    "chalk": "^5.6.2",
